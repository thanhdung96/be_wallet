{% extends 'base.html.twig' %}

{% block title %}DefaultCategory{% endblock %}

{% block body %}
    <h1>DefaultCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ default_category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ default_category.name }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ default_category.color }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ default_category.type }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ default_category.active }}</td>
            </tr>
            <tr>
                <th>Ordering</th>
                <td>{{ default_category.ordering }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ default_category.icon }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('default_category_index') }}">back to list</a>

    <a href="{{ path('default_category_edit', {'id': default_category.id}) }}">edit</a>

    {{ include('default_category/_delete_form.html.twig') }}
{% endblock %}
